   "source": [
    "# Сохраняем граф в файл при помощи модуля pickle\n",
    "import pickle\n",
    "with open('graph.pickle', 'wb') as f:\n",
    "    pickle.dump(G, f)"
   ]
  },
   ],
   "source": [
    "pos = nx.spring_layout(G)\n",
    "nx.draw(G, pos, node_color='lightblue', with_labels=True, font_size=10, node_size=500, alpha=0.8)"
   ]
  },
  {
